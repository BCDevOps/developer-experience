{
  "name": "requestron",
  "version": "1.0.0",
  "description": "A Probot app to handle DevOps requests",
  "author": "ShellyXueHan <shelly.han@gov.bc.ca>",
  "license": "Apache-2.0",
  "repository": "https://github.com/BCDevOps/developer-experience.git",
  "homepage": "https://github.com/BCDevOps/developer-experience",
  "bugs": "https://github.com/BCDevOps/developer-experience/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon --exec \"npm start\"",
    "start": "probot run ./lib/index.js",
    "lint": "standard **/*.ts --fix",
    "test": "jest && standard **/*.ts",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@octokit/auth-app": "^2.10.1",
    "acorn": "^7.1.0",
    "axios": "^0.21.1",
    "json-web-token": "^3.2.0",
    "octokit": "^1.0.0-hello-world",
    "probot": "^9.15.1",
    "probot-scheduler": "^2.0.0-beta.1",
    "smee-client": "^1.0.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/nock": "^11.1.0",
    "@types/node": "^12.0.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^24.0.0",
    "nock": "^11.6.0",
    "nodemon": "^1.17.2",
    "standard": "^12.0.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.1",
    "typescript-eslint-parser": "^22.0.0"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "parser": "typescript-eslint-parser",
    "env": [
      "jest"
    ],
    "plugins": [
      "typescript"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
