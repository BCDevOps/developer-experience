  },
  "engines" : {
    "node" : ">=6"
  },
  "main" : "index.js",
  "dist" : {
    "tarball" : "https://artifacts.developer.gov.bc.ca/artifactory/api/npm//npm-remote-cache/package-hash/-/package-hash-3.0.0.tgz",
    "shasum" : "50183f2d36c9e3e528ea0a8605dff57ce976f88e"
  },
  "dependencies" : {
