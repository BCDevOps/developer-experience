{
  "name" : "node-notifier",
  "description" : "A Node.js module for sending notifications on native Mac, Windows (post and pre 8) and Linux (or Growl as fallback)",
  "version" : "6.0.0",
  "author" : "Mikael Brevik",
  "homepage" : "https://github.com/mikaelbr/node-notifier#readme",
  "repository" : {
    "type" : "git",
    "url" : "git+ssh://git@github.com/mikaelbr/node-notifier.git"
  },
  "bugs" : {
    "url" : "https://github.com/mikaelbr/node-notifier/issues"
  },
  "main" : "index.js",
  "dist" : {
    "tarball" : "https://artifacts.developer.gov.bc.ca/artifactory/api/npm//npm-remote-cache/node-notifier/-/node-notifier-6.0.0.tgz",
    "shasum" : "cea319e06baa16deec8ce5cd7f133c4a46b68e12"
  },
  "dependencies" : {
    "growly" : "^1.3.0",
    "is-wsl" : "^2.1.1",
    "semver" : "^6.3.0",
    "shellwords" : "^0.1.1",
    "which" : "^1.3.1"
  },
  "devDependencies" : {
    "eslint" : "^6.4.0",
    "eslint-config-semistandard" : "^15.0.0",
    "eslint-config-standard" : "^14.1.0",
    "eslint-plugin-import" : "^2.18.2",
    "eslint-plugin-node" : "^10.0.0",
    "eslint-plugin-promise" : "^4.2.1",
    "eslint-plugin-standard" : "^4.0.1",
    "husky" : "^3.0.5",
    "jest" : "^24.9.0",
    "lint-staged" : "^9.3.0",
    "prettier" : "^1.18.2"
  },
  "keywords" : [ "notification center", "mac os x 10.8", "notify", "terminal-notifier", "notify-send", "growl", "windows 8 notification", "toaster", "notification" ],
  "license" : "MIT",
  "directories" : {
    "example" : "example",
    "test" : "test"
  },
  "scripts" : {
    "pretest" : "npm run lint",
    "test" : "jest",
    "example" : "node ./example/message.js",
    "example:mac" : "node ./example/advanced.js",
    "example:mac:input" : "node ./example/macInput.js",
    "example:windows" : "node ./example/toaster.js",
    "lint" : "eslint example/*.js lib/*.js notifiers/*.js test/**/*.js index.js"
  },
  "lint-staged" : {
    "*.{js,json,css,md}" : [ "prettier --write", "git add" ]
  },
  "husky" : {
    "hooks" : {
      "pre-commit" : "lint-staged"
    }
  },
  "jest" : {
    "testRegex" : "/test/[^_]*.js",
    "testEnvironment" : "node",
    "setupFilesAfterEnv" : [ "./test/_test-matchers.js" ]
  }
}