{
  "name" : "@sinonjs/formatio",
  "description" : "Human-readable object formatting",
  "version" : "3.2.1",
  "author" : "Christian Johansen",
  "homepage" : "https://sinonjs.github.io/formatio/",
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/sinonjs/formatio.git"
  },
  "main" : "./lib/formatio",
  "dist" : {
    "tarball" : "https://artifacts.developer.gov.bc.ca/artifactory/api/npm//npm-remote-cache/@sinonjs/formatio/-/formatio-3.2.1.tgz",
    "shasum" : "52310f2f9bcbc67bdac18c94ad4901b95fde267e"
  },
  "dependencies" : {
    "@sinonjs/commons" : "^1",
    "@sinonjs/samsam" : "^3.1.0"
  },
  "devDependencies" : {
    "@sinonjs/referee" : "^2.6.0",
    "eslint" : "^4.19.1",
    "eslint-config-sinon" : "^1.0.3",
    "eslint-plugin-ie11" : "^1.0.0",
    "eslint-plugin-mocha" : "^4.11.0",
    "mocha" : "^5.0.0",
    "nyc" : "^11.7.3",
    "rollup" : "0.65.2",
    "rollup-plugin-commonjs" : "9.1.6"
  },
  "license" : "BSD-3-Clause",
  "files" : [ "lib/**/*[^test].js" ],
  "scripts" : {
    "build" : "npm run build:dist-folder && npm run build:bundle",
    "build:bundle" : "rollup -c > dist/formatio.js",
    "build:dist-folder" : "mkdirp dist",
    "lint" : "eslint .",
    "prepublishOnly" : "npm run build && mkdocs gh-deploy -r upstream || mkdocs gh-deploy -r origin",
    "test" : "mocha 'lib/**/*.test.js'",
    "test-coverage" : "nyc --reporter text --reporter html --reporter lcovonly npm run test"
  }
}