{
  "name" : "saslprep",
  "description" : "SASLprep: Stringprep Profile for User Names and Passwords, rfc4013.",
  "version" : "1.0.3",
  "author" : "Dmitry Tsvettsikh <me@reklatsmasters.com>",
  "homepage" : "https://github.com/reklatsmasters/saslprep#readme",
  "repository" : {
    "type" : "git",
    "url" : "git+https://github.com/reklatsmasters/saslprep.git"
  },
  "bugs" : {
    "url" : "https://github.com/reklatsmasters/saslprep/issues"
  },
  "engines" : {
    "node" : ">=6"
  },
  "main" : "index.js",
  "dist" : {
    "tarball" : "https://artifacts.developer.gov.bc.ca/artifactory/api/npm//npm-remote-cache/saslprep/-/saslprep-1.0.3.tgz",
    "shasum" : "4c02f946b56cf54297e347ba1093e7acac4cf226"
  },
  "dependencies" : {
    "sparse-bitfield" : "^3.0.3"
  },
  "devDependencies" : {
    "@nodertc/eslint-config" : "^0.2.1",
    "eslint" : "^5.16.0",
    "jest" : "^23.6.0",
    "prettier" : "^1.14.3"
  },
  "keywords" : [ "sasl", "saslprep", "stringprep", "rfc4013", "4013" ],
  "license" : "MIT",
  "scripts" : {
    "test" : "npm run lint && npm run unit-test",
    "lint" : "npx eslint --quiet .",
    "unit-test" : "npx jest",
    "gen-code-points" : "node generate-code-points.js > code-points.mem"
  },
  "eslintConfig" : {
    "extends" : "@nodertc",
    "rules" : {
      "camelcase" : "off",
      "no-continue" : "off"
    },
    "overrides" : [ {
      "files" : [ "test/*.js" ],
      "env" : {
        "jest" : true
      },
      "rules" : {
        "require-jsdoc" : "off"
      }
    } ]
  },
  "jest" : {
    "modulePaths" : [ "<rootDir>" ],
    "testMatch" : [ "**/test/*.js" ],
    "testPathIgnorePatterns" : [ "<rootDir>/node_modules/" ]
  }
}