  },
  "engines" : {
    "node" : ">= 8.0.0"
  },
  "main" : "dist/mixme.cjs.js",
  "dist" : {
    "tarball" : "https://artifacts.developer.gov.bc.ca/artifactory/api/npm//npm-remote-cache/mixme/-/mixme-0.3.5.tgz",
    "shasum" : "304652cdaf24a3df0487205e61ac6162c6906ddd"
  },
  "devDependencies" : {
    "@babel/core" : "^7.4.3",
    "@babel/preset-env" : "^7.4.3",
