  "contributors" : [ "Blaine Bublitz (https://twitter.com/BlaineBublitz)", "Jon Schlinkert (http://twitter.com/jonschlinkert)" ],
  "bugs" : {
    "url" : "https://github.com/jonschlinkert/normalize-path/issues"
  },
  "engines" : {
    "node" : ">=0.10.0"
  },
  "main" : "index.js",
  "dist" : {
    "tarball" : "https://artifacts.developer.gov.bc.ca/artifactory/api/npm//npm-remote-cache/normalize-path/-/normalize-path-3.0.0.tgz",
    "shasum" : "0dcd69ff23a1c9b11fd0978316644a0388216a65"
  },
  "devDependencies" : {
