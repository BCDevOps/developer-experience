{
  "name" : "tsutils",
  "description" : "utilities for working with typescript's AST",
  "version" : "2.28.0",
  "author" : "Klaus Meinhardt",
  "repository" : {
    "type" : "git",
    "url" : "https://github.com/ajafff/tsutils"
  },
  "dist" : {
    "tarball" : "https://artifacts.developer.gov.bc.ca/artifactory/api/npm//npm-remote-cache/tsutils/-/tsutils-2.28.0.tgz",
    "shasum" : "6bd71e160828f9d019b6f4e844742228f85169a1"
  },
  "dependencies" : {
    "tslib" : "^1.8.1"
  },
  "devDependencies" : {
    "@fimbul/valtyr" : "^0.12.0",
    "@fimbul/wotan" : "^0.12.0",
    "@types/chai" : "^4.0.10",
    "@types/mocha" : "^5.0.0",
    "@types/node" : "^10.0.3",
    "chai" : "^4.1.2",
    "coveralls" : "^3.0.0",
    "github-release-from-changelog" : "^1.3.0",
    "mocha" : "^5.0.0",
    "npm-run-all" : "^4.1.2",
    "nyc" : "^12.0.1",
    "tslint" : "^5.8.0",
    "tslint-consistent-codestyle" : "^1.11.0",
    "typescript" : "^3.0.0-rc"
  },
  "peerDependencies" : {
    "typescript" : ">=2.1.0 || >=2.1.0-dev || >=2.2.0-dev || >=2.3.0-dev || >=2.4.0-dev || >=2.5.0-dev || >=2.6.0-dev || >=2.7.0-dev || >=2.8.0-dev || >=2.9.0-dev || >= 3.0.0-dev || >= 3.1.0-dev"
  },
  "keywords" : [ "typescript", "ts", "ast", "typeguard", "utils", "helper", "node" ],
  "license" : "MIT",
  "scripts" : {
    "compile" : "rm -rf {,util,typeguard,test/**}/*.js; tsc -p .",
    "lint:tslint" : "wotan -m @fimbul/valtyr",
    "lint:wotan" : "wotan",
    "lint" : "run-p lint:*",
    "test" : "mocha test/*Tests.js && tslint --test 'test/rules/**/tslint.json'",
    "verify" : "run-s compile lint coverage",
    "prepublishOnly" : "npm run verify",
    "coverage" : "nyc npm test",
    "report-coverage" : "cat ./coverage/lcov.info | coveralls",
    "github-release" : "GITHUB_TOKEN=$(cat ~/github_token.txt) github-release-from-changelog",
    "postpublish" : "git push origin master --tags; npm run github-release"
  }
}