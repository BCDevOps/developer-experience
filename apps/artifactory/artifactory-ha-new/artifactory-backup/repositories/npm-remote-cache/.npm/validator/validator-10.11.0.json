  "main" : "index.js",
  "dist" : {
    "tarball" : "https://artifacts.developer.gov.bc.ca/artifactory/api/npm//npm-remote-cache/validator/-/validator-10.11.0.tgz",
    "shasum" : "003108ea6e9a9874d31ccc9e5006856ccd76b228"
  },
  "devDependencies" : {
    "@babel/cli" : "^7.0.0",
    "@babel/core" : "^7.0.0",
    "@babel/preset-env" : "^7.0.0",
    "@babel/register" : "^7.0.0",
    "babel-eslint" : "^10.0.1",
    "babel-plugin-add-module-exports" : "^1.0.0",
    "eslint" : "^4.19.1",
    "eslint-config-airbnb-base" : "^12.1.0",
    "eslint-plugin-import" : "^2.11.0",
    "mocha" : "^5.1.1",
    "rollup" : "^0.43.0",
    "rollup-plugin-babel" : "^4.0.1",
    "uglify-js" : "^3.0.19"
  },
  "keywords" : [ "validator", "validation", "validate", "sanitization", "sanitize", "sanitisation", "sanitise", "assert" ],
