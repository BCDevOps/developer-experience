{
  "name" : "v8-to-istanbul",
  "description" : "convert from v8 coverage format to istanbul's format",
  "version" : "4.1.3",
  "author" : "Ben Coe <ben@npmjs.com>",
  "repository" : "istanbuljs/v8-to-istanbul",
  "engines" : {
    "node" : "8.x.x || >=10.10.0"
  },
  "main" : "index.js",
  "dist" : {
    "tarball" : "https://artifacts.developer.gov.bc.ca/artifactory/api/npm//npm-remote-cache/v8-to-istanbul/-/v8-to-istanbul-4.1.3.tgz",
    "shasum" : "22fe35709a64955f49a08a7c7c959f6520ad6f20"
  },
  "dependencies" : {
    "@types/istanbul-lib-coverage" : "^2.0.1",
    "convert-source-map" : "^1.6.0",
    "source-map" : "^0.7.3"
  },
  "devDependencies" : {
    "@types/node" : "^12.7.11",
    "c8" : "^7.0.0",
    "coveralls" : "3.0.11",
    "should" : "13.2.3",
    "standard" : "^14.3.1",
    "tap" : "^14.9.2"
  },
  "keywords" : [ "istanbul", "v8", "coverage" ],
  "license" : "ISC",
  "files" : [ "lib/*.js", "index.js", "index.d.ts" ],
  "scripts" : {
    "fix" : "standard --fix",
    "snapshot" : "TAP_SNAPSHOT=1 tap test/*.js",
    "test" : "c8 --reporter=html --reporter=text tap --no-coverage --no-esm test/*.js",
    "posttest" : "standard",
    "coverage" : "c8 report --reporter=text-lcov | coveralls"
  },
  "standard" : {
    "ignore" : [ "**/test/fixtures" ]
  },
  "types" : "index.d.ts"
}