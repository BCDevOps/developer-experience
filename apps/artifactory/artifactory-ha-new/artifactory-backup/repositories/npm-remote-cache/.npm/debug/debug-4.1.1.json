{
  "name" : "debug",
  "description" : "small debugging utility",
  "version" : "4.1.1",
  "author" : "TJ Holowaychuk <tj@vision-media.ca>",
  "repository" : {
    "type" : "git",
    "url" : "git://github.com/visionmedia/debug.git"
  },
  "contributors" : [ "Nathan Rajlich <nathan@tootallnate.net> (http://n8.io)", "Andrew Rhyne <rhyneandrew@gmail.com>" ],
  "main" : "./src/index.js",
  "dist" : {
    "tarball" : "https://artifacts.developer.gov.bc.ca/artifactory/api/npm//npm-remote-cache/debug/-/debug-4.1.1.tgz",
    "shasum" : "3b72260255109c6b589cee050f1d516139664791"
  },
  "dependencies" : {
    "ms" : "^2.1.1"
  },
  "devDependencies" : {
    "@babel/cli" : "^7.0.0",
    "@babel/core" : "^7.0.0",
    "@babel/preset-env" : "^7.0.0",
    "browserify" : "14.4.0",
    "chai" : "^3.5.0",
    "concurrently" : "^3.1.0",
    "coveralls" : "^3.0.2",
    "istanbul" : "^0.4.5",
    "karma" : "^3.0.0",
    "karma-chai" : "^0.1.0",
    "karma-mocha" : "^1.3.0",
    "karma-phantomjs-launcher" : "^1.0.2",
    "mocha" : "^5.2.0",
    "mocha-lcov-reporter" : "^1.2.0",
    "rimraf" : "^2.5.4",
    "xo" : "^0.23.0"
  },
  "keywords" : [ "debug", "log", "debugger" ],
  "license" : "MIT",
  "files" : [ "src", "dist/debug.js", "LICENSE", "README.md" ],
  "scripts" : {
    "lint" : "xo",
    "test" : "npm run test:node && npm run test:browser",
    "test:node" : "istanbul cover _mocha -- test.js",
    "pretest:browser" : "npm run build",
    "test:browser" : "karma start --single-run",
    "prebuild:debug" : "mkdir -p dist && browserify --standalone debug -o dist/debug.es6.js .",
    "build:debug" : "babel -o dist/debug.js dist/debug.es6.js > dist/debug.js",
    "build:test" : "babel -d dist test.js",
    "build" : "npm run build:debug && npm run build:test",
    "clean" : "rimraf dist coverage",
    "test:coverage" : "cat ./coverage/lcov.info | coveralls"
  },
  "browser" : "./src/browser.js",
  "unpkg" : "./dist/debug.js"
}