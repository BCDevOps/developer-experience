{
  "name" : "each-props",
  "description" : "Processes each properties of an object deeply.",
  "version" : "1.3.2",
  "author" : "Takayuki Sato",
  "homepage" : "https://github.com/sttk/each-props#readme",
  "repository" : {
    "type" : "git",
    "url" : "git+https://github.com/sttk/each-props.git"
  },
  "bugs" : {
    "url" : "https://github.com/sttk/each-props/issues"
  },
  "main" : "index.js",
  "dist" : {
    "tarball" : "https://artifacts.developer.gov.bc.ca/artifactory/api/npm//npm-remote-cache/each-props/-/each-props-1.3.2.tgz",
    "shasum" : "ea45a414d16dd5cfa419b1a81720d5ca06892333"
  },
  "dependencies" : {
    "is-plain-object" : "^2.0.1",
    "object.defaults" : "^1.1.0"
  },
  "devDependencies" : {
    "browserify" : "^16.2.2",
    "chai" : "^3.5.0",
    "coveralls" : "^3.0.1",
    "eslint" : "^4.19.1",
