{
  "name" : "object-inspect",
  "description" : "string representations of objects in node and the browser",
  "version" : "1.7.0",
  "author" : {
    "name" : "James Halliday",
    "email" : "mail@substack.net",
    "url" : "http://substack.net"
  },
  "homepage" : "https://github.com/substack/object-inspect",
  "repository" : {
    "type" : "git",
    "url" : "git://github.com/substack/object-inspect.git"
  },
  "main" : "index.js",
  "dist" : {
    "tarball" : "https://artifacts.developer.gov.bc.ca/artifactory/api/npm//npm-remote-cache/object-inspect/-/object-inspect-1.7.0.tgz",
    "shasum" : "f4f6bd181ad77f006b5ece60bd0b6f398ff74a67"
  },
  "devDependencies" : {
    "@ljharb/eslint-config" : "^15.0.1",
    "core-js" : "^2.6.10",
    "eslint" : "^6.6.0",
    "nyc" : "^10.3.2",
    "tape" : "^4.11.0"
  },
  "keywords" : [ "inspect", "util.inspect", "object", "stringify", "pretty" ],
  "license" : "MIT",
  "scripts" : {
    "pretest" : "npm run lint",
    "lint" : "eslint .",
    "test" : "npm run tests-only",
    "pretests-only" : "node test-core-js",
    "tests-only" : "tape test/*.js",
    "posttest" : "npx aud --production",
    "coverage" : "nyc npm run tests-only"
  },
  "funding" : {
    "url" : "https://github.com/sponsors/ljharb"
  },
  "greenkeeper" : {
    "ignore" : [ "nyc", "core-js" ]
  },
  "browser" : {
    "./util.inspect.js" : false
  },
  "testling" : {
    "files" : [ "test/*.js", "test/browser/*.js" ],
