{
  "name" : "@octokit/types",
  "description" : "Shared TypeScript definitions for Octokit projects",
  "version" : "2.5.1",
  "author" : "Gregor Martynus (https://twitter.com/gr2m)",
  "repository" : "https://github.com/octokit/types.ts",
  "main" : "src/index.ts",
  "dist" : {
    "tarball" : "https://artifacts.developer.gov.bc.ca/artifactory/api/npm//npm-remote-cache/@octokit/types/-/types-2.5.1.tgz",
    "shasum" : "22563b3bb50034bea3176eac1860340c5e812e2a"
  },
  "dependencies" : {
    "@types/node" : ">= 8"
  },
  "devDependencies" : {
    "@octokit/graphql" : "^4.2.2",
    "handlebars" : "^4.4.5",
    "lodash.set" : "^4.3.2",
    "npm-run-all" : "^4.1.5",
    "pascal-case" : "^3.1.1",
    "prettier" : "^2.0.0",
    "semantic-release" : "^17.0.0",
    "semantic-release-plugin-update-version-in-files" : "^1.0.0",
    "sort-keys" : "^4.0.0",
    "string-to-jsdoc-comment" : "^1.0.0",
    "typedoc" : "^0.17.0",
    "typescript" : "^3.6.4"
  },
  "keywords" : [ "github", "api", "sdk", "toolkit", "typescript" ],
  "license" : "MIT",
  "files" : [ "src/" ],
  "scripts" : {
    "docs" : "typedoc --module commonjs --readme none --out docs src/",
    "lint" : "prettier --check '{src,test}/**/*' README.md package.json !src/generated/*",
    "lint:fix" : "prettier --write '{src,test}/**/*' README.md package.json !src/generated/*",
    "pretest" : "npm run -s lint",
    "test" : "./node_modules/.bin/tsc --noEmit --declaration src/index.ts",
    "update-endpoints" : "npm-run-all update-endpoints:*",
    "update-endpoints:fetch-json" : "node scripts/update-endpoints/fetch-json",
    "update-endpoints:typescript" : "node scripts/update-endpoints/typescript"
  },
  "publishConfig" : {
    "access" : "public"
  },
  "release" : {
    "plugins" : [ "@semantic-release/commit-analyzer", "@semantic-release/release-notes-generator", "@semantic-release/github", "@semantic-release/npm", [ "semantic-release-plugin-update-version-in-files", {
      "files" : [ "src/VERSION.ts" ]
    } ] ]
  }
}